{
  "version": 2,
  "builds": [
    {
      "src": "nuxt.config.js",
      "use": "@nuxtjs/vercel-builder",
      "config": {
        "serverFiles": ["server-middleware/**"]
      }
    },
    {
      "src": "api/**/*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/getArrInfoByStopID", "dest": "/api/getArrInfoByStopID/index.ts" },
    {
      "src": "/api/getArrInfoByStopID/fetchBusStopData",
      "dest": "/api/getArrInfoByStopID/fetchBusStopData.ts"
    }
  ]
}
